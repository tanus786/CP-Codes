int maxSubArray(vector<int>& nums) {
        int l=nums.size();
        int max=INT_MIN;
        int sum=0;
            
        for(int i=0;i<l;i++){
            
             sum=sum+nums[i];
                if(sum>max){
                    max=sum;
                }
               if(sum<0){
                    sum=0;
                }
        
        }
        return max;
    }
